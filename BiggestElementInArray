public class BiggestElementInArray {
    int arr[]=new int[5];
    int temp=0;
    public void biggestelement(){
        temp=arr[0];
        for (int i = 0; i <arr.length-1 ; i++) {
            if (temp < arr[i + 1]) {
                temp = arr[i+1];
            }
        }
        System.out.println(temp);
    }
}
